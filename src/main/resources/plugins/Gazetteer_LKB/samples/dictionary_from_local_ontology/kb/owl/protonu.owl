    <rdfs:domain rdf:resource="&pupp;PublicCompany"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="&ptop;hasDate"/>
  <owl:DatatypeProperty rdf:about="&ptop;informationResourceIdentifier"/>

